  "name": "assembly-script-simple",
  "version": "1.0.0",
  "description": "Simple AssemblyScript demo",
  "main": "index.js",
  "scripts": {
    "test": "node tests",
    "start": "browser-sync start --server --files ./**/* --ss ./src",
